<apex:page controller="CustomerRegistrationController">
 	<apex:form >
    	<apex:pageBlock title="Customer Registration">
        	<apex:pageBlockSection columns="1">
            	<apex:inputField value="{!objA.Name}"/>
                <apex:inputField value="{!objA.Phone}"/>
                <apex:inputField value="{!objA.Site}"/>
                <apex:inputField value="{!objA.SLA__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
        <apex:commandButton value="Save" action="{!doSave}" style="margin-left:20%;"/>
    </apex:form>
  
</apex:page>